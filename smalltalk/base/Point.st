EVAL Class addNewClass: ( Magnitude subclass: 'Point' variables: 'x y ' classVariables: '')
METHOD Point
+ d  " add two points "
  ^ (x + d x) @ (y + d y)
!
METHOD Point
printString
   ^ x printString + '@' + y printString
!
METHOD Point
x
 ^ x
!
METHOD Point
x: ix
  x <- ix
!
METHOD Point
x: ix y: iy
  x <- ix. y <- iy
!
METHOD Point
y
 ^ y
!
METHOD Point
y: iy
   y <- iy
!
